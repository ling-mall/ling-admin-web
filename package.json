{
  "name": "ling-admin-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "test": "",
    "dev": "vite",
    "build": "cross-env NODE_ENV=production vite build && esno ./build/script/postBuild.ts",
    "preview": "vite preview",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "prepare": "husky install",
    "lint:pretty": "pretty-quick --staged",
    "lint:lint-staged": "lint-staged",
    "changelog": "conventional-changelog -p custom-config -i CHANGELOG.md -s -r 0  -n ./changelog-option.cjs",
    "release": "release-it --no-npm",
    "commit": "cz-customizable"
  },
  "dependencies": {
    "@iconify/iconify": "^3.1.1",
    "@unocss/reset": "^0.58.3",
    "axios": "^1.6.2",
    "element-plus": "^2.4.3",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.0",
    "primevue": "^3.45.0",
    "qs": "^6.11.2",
    "vue": "^3.3.10",
    "vue-i18n": "^9.8.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@iconify/json": "^2.2.153",
    "@purge-icons/generated": "^0.10.0",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/qs": "^6.9.10",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue-macros/volar": "^0.17.4",
    "@vueuse/core": "^10.7.0",
    "@vueuse/router": "^10.7.0",
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^4.1.0",
    "conventional-changelog-custom-config": "^0.3.1",
    "cross-env": "^7.0.3",
    "cz-customizable": "^7.0.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.0.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.19.2",
    "esno": "^4.0.0",
    "fs-extra": "^11.2.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.32",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.1.0",
    "release-it": "^17.0.0",
    "sass": "^1.69.5",
    "stylelint": "^15.11.0",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.3.1",
    "typescript": "^5.3.3",
    "unocss": "^0.57.7",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-vue-components": "^0.25.2",
    "unplugin-vue-macros": "^2.7.0",
    "vite": "^5.0.6",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-purge-icons": "^0.10.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-eslint-parser": "^9.3.2",
    "vue-tsc": "^1.8.25"
  },
  "config": {
    "cz-customizable": {
      "config": ".cz-config.cjs"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,cjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write --parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,less,styl,html}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ],
    ".czrc": [
      "prettier --write --parser json"
    ]
  },
  "volta": {
    "node": "18.18.2"
  }
}
